    "@type" : "owl:DatatypeProperty",
    "comment" : {
      "@language" : "en-gb",
      "@value" : "The database name is used to adress the correct mapping from the mapping file to a certain database within the connection string."
    },
    "domain" : "exdoc:ExternalDatabase",
    "label" : {
      "@language" : "en-gb",
      "@value" : "database name"
    },
    "range" : "xsd:string",
